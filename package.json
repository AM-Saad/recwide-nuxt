{
   "private": true,
   "type": "module",
   "author": "Abdelrahman Saad",
   "scripts": {
      "build": "nuxt build --preset=vercel_edge",
      "dev": "nuxt dev --port 8004",
      "generate": "nuxt generate",
      "preview": "nuxt preview",
      "postinstall": "nuxt prepare",
      "typecheck": "nuxt typecheck",
      "start": "nuxt start",
      "sitemap": "vue-cli-service sitemap",
      "db:sync": "prisma generate",
      "prepare": "husky install",
      "lint": "yarn lint:eslint && yarn lint:prettier",
      "lint:eslint": "eslint .",
      "lint:prettier": "prettier . --check",
      "lintfix": "yarn lint:eslint --fix && yarn lint:prettier --write",
      "format": "prettier --write \"{components,pages,plugins,middleware,layouts,composables,assets}/**/*.{js,jsx,ts,tsx,vue,html,css,scss,json,md}\"",
      "pre-commit": "husky install && lint-staged"
   },
   "devDependencies": {
      "@commitlint/cli": "^18.5.0",
      "@commitlint/config-conventional": "^18.5.0",
      "@ffmpeg/core": "0.11.0",
      "@nuxt/eslint": "^0.5.0",
      "@nuxt/image": "^1.4.0",
      "@nuxtjs/eslint-config-typescript": "^12.1.0",
      "@nuxtjs/tailwindcss": "^6.8.0",
      "@sidebase/nuxt-auth": "^0.9.0",
      "@sidebase/nuxt-session": "^0.2.8",
      "@simplewebauthn/types": "^10.0.0",
      "@types/bcrypt": "^5.0.2",
      "@types/jsonwebtoken": "^9.0.5",
      "@types/node": "^18.11.18",
      "@types/socket.io-client": "^3.0.0",
      "autoprefixer": "^10.4.17",
      "commitizen": "^4.3.0",
      "eslint": "^9.9.0",
      "eslint-plugin-vue": "^9.27.0",
      "globals": "^15.9.0",
      "husky": "^9.1.4",
      "lint-staged": "^15.2.8",
      "prettier": "^3.3.3",
      "prettier-plugin-tailwindcss": "0.2.7",
      "prisma": "4.8.1",
      "typescript": "^5.5.4",
      "vite-plugin-cross-origin-isolation": "^0.1.6",
      "vue-tsc": "^2.1.6"
   },
   "dependencies": {
      "@auth/core": "^0.34.2",
      "@ffmpeg/ffmpeg": "0.11.5",
      "@headlessui/vue": "^1.7.17",
      "@heroicons/vue": "^2.1.1",
      "@nuxt/ui": "^2.18.7",
      "@pinia/nuxt": "^0.5.1",
      "@prisma/client": "4.8.1",
      "@sendgrid/mail": "^8.1.3",
      "@simplewebauthn/browser": "^10.0.0",
      "@simplewebauthn/server": "^10.0.1",
      "@types/firefox-webext-browser": "^120.0.2",
      "@vite-pwa/nuxt": "^0.5.0",
      "bcryptjs": "^2.4.3",
      "core-js": "3.25.5",
      "jsonwebtoken": "^9.0.2",
      "mongodb": "5.3.0",
      "nanoid": "4.0.2",
      "next-auth": "4.21.1",
      "nuxt": "^3.10.2",
      "pinia": "^2.1.7",
      "postcss-custom-properties": "^13.3.4",
      "socket.io-client": "^4.6.1",
      "superjson": "^1.12.2",
      "vue": "^3.4.19",
      "vue-disable-autocomplete": "^0.0.4",
      "web-push": "^3.6.7",
      "zod": "^3.22.4"
   },
   "config": {
      "commitizen": {
         "path": "cz-conventional-changelog"
      }
   },
   "lint-staged": {
      "*.vue": [
         "eslint --fix",
         "git add"
      ],
      "*.js": [
         "eslint --fix",
         "git add"
      ],
      "*.ts": [
         "eslint --fix",
         "git add"
      ]
   }
}