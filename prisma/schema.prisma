generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type ProjectsMedia {
  extension String
  name      String
  url       String
}

type SessionsSession {
  challenge String?
  cookie    SessionsSessionCookie
  email     String
}

type SessionsSessionCookie {
  /// Could not determine type: the field only had null or empty values in the sample set.
  domain         Json?
  /// Could not determine type: the field only had null or empty values in the sample set.
  expires        Json?
  httpOnly       Boolean
  /// Could not determine type: the field only had null or empty values in the sample set.
  originalMaxAge Json?
  path           String
  /// Could not determine type: the field only had null or empty values in the sample set.
  sameSite       Json?
  /// Could not determine type: the field only had null or empty values in the sample set.
  secure         Json?
}

type SubscriptionKeys {
  auth   String
  p256dh String
}

model Subscription {
  id       String           @id @default(auto()) @map("_id") @db.ObjectId
  endpoint String
  keys     SubscriptionKeys
}

model projects {
  id            String          @id @default(auto()) @map("_id") @db.ObjectId
  audioSettings String
  date          String
  media         ProjectsMedia[]
  mode          String
  name          String
  slug          String
  user          String          @db.ObjectId
}

model publickeycreds {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  backed_up       Boolean
  passkey_user_id String   @db.ObjectId
  public_key      Bytes?
  credentialID    String
  transports      String[]
  counter         Int
}

model sessions {
  id      String          @id @map("_id")
  expires DateTime        @db.Date
  session SessionsSession

  @@index([expires], map: "expires_1")
}

model users {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  credentials     Json?
  email           String  @unique
  name            String
  passkey_user_id String?  @db.ObjectId
  password        String
  projects        String[] @db.ObjectId
}
